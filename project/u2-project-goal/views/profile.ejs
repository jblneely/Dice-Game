<% aims.forEach(function(aim) { %>
	<div class="well">
	  <h2><a href="/profile/<%= aim.id %>"><%= aim.objective %></a>
	    <small><%= aim.objective %></small>
	  </h2>

	  <div class="radio">
	  <label><input type="radio" name="optradio">done - add one</label>
	</div>
	<div class="radio">
	  <label><input type="radio" name="optradio">not done - add none</label>
	</div>

	  <a href="/profile/<%= aim.id %>/edit" class="btn btn-info glyphicon glyphicon-edit"></a>

	  <a href="/profile/<%= aim.id %>" class="btn btn-danger glyphicon glyphicon-trash delete-btn"></a>

	</div>
<% }); %>