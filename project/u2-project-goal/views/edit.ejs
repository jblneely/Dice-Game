<h1>Edit</h1>

<form action="/profile/<%= aim.id %>" class="edit-form" role="form">
  <div class="form-group">
    <label for="name">AIM</label>
    <input type="text" class="form-control" id="name" placeholder="Objective" name="name" value="<%= aim.objective %>">
  </div>
  <div class="form-group">
    <label for="name">FIRE</label>
    <input type="text" class="form-control" id="name" placeholder="Key Result" name="name" value="<%= fire.keyResult %>">
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<a href="/profile">&larr; Return to Profile</a>
